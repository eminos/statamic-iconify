{
    "name": "eminos/statamic-iconify",
    "type": "statamic-addon",
    "description": "A Statamic addon to use Iconify icons in your Statamic site.",
    "license": "MIT",
    "require": {
        "statamic/cms": "^4.0"
    },
    "require-dev": {
        "php": "^8.1",
        "laravel/framework": "^10.8",
        "statamic/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "StatamicIconify\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Iconify",
            "description": "A Statamic addon to use Iconify icons in your Statamic site."
        },
        "laravel": {
            "providers": [
                "StatamicIconify\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
